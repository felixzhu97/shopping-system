# GitHub Actions Secrets for local testing
# Copy this file to .secrets and fill in your values
# Usage: act --secret-file .secrets

# AWS Credentials (for deploy workflows)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# MongoDB URIs
MONGODB_URI_TEST=your_mongodb_test_uri
MONGODB_URI_PROD=your_mongodb_prod_uri

# JWT Secrets
JWT_SECRET_TEST=your_jwt_secret_test
JWT_SECRET_PROD=your_jwt_secret_prod

# CORS Origins
CORS_ORIGINS_TEST=your_cors_origins_test
CORS_ORIGINS_PROD=your_cors_origins_prod

# Datadog
DD_API_KEY=your_datadog_api_key
DD_SITE=datadoghq.com
DD_LAYER_VERSION=119
