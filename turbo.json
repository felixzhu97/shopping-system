{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "remoteCache": {
    "enabled": true
  },
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {},
    "lint:fix": {},
    "format": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "start": {
      "dependsOn": [
        "^build"
      ],
      "cache": false,
      "persistent": true
    },
    "storybook": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "test:coverage": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "coverage/**"
      ]
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "seed:products200:dev": {
      "cache": false,
      "outputs": []
    },
    "seed:products200:test": {
      "cache": false,
      "outputs": []
    },
    "seed:products200:prod": {
      "cache": false,
      "outputs": []
    },
    "seed:orders-users:dev": {
      "cache": false,
      "outputs": []
    },
    "seed:orders-users:test": {
      "cache": false,
      "outputs": []
    },
    "seed:orders-users:prod": {
      "cache": false,
      "outputs": []
    }
  }
}