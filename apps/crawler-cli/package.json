{
  "name": "crawler-cli",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc --skipLibCheck && pnpm run bundle",
    "bundle": "esbuild src/index.ts --bundle --platform=node --target=node20 --outfile=dist/index.js",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "cheerio": "^1.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.17.30",
    "esbuild": "^0.25.0",
    "eslint": "^8.48.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "pnpm": "^10.1.0",
    "node": ">=20.0.0"
  }
}
